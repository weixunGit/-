page{  
  height: 100%;  
}  
.container {  
  height: 100%;  
  display: flex;  
  flex-direction: column;  
  padding: 0;  
  box-sizing: border-box;  
  background-color: #f2f2f2;
}   
/*表单内容*/  
.from {  
  margin-top: 180px;  
  flex: auto;  
  height:100%;
}  
.inputView {  
  background-color: #fff;  
  line-height: 44px;
}  
/*输入框*/  
.keyImage {  
  margin-left: 22px;  
  width: 14px;  
  height: 14px  
}  
.inputText {  
  flex: block;  
  float: right;  
  text-align: right;  
  margin-right: 22px;  
  margin-top: 11px;  
  color: #cccccc;  
  font-size:14px;
  width:220px;
}  
.line {  
  width: 100%;  
  height: 1px;  
  background-color: #cccccc;  
  margin-top: 1px;  
}  
.OKbtn{
  width:40%;
  float: left;
  border-radius: 38rpx;
  padding: 18rpx 0;
  margin-top: 35px;
}
.NObtn{
  width:40%;
  float: right;
  border-radius: 38rpx;
  padding: 18rpx 0;
  margin-top: 35px;
}